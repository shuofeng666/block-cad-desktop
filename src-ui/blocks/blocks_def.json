            "value": "0"
          },
          {
            "name": "rotate_y",
            "type": "num",
            "value": "0"
          },
          {
            "name": "rotate_z",
            "type": "num",
            "value": "0"
          }
        ]
      },
      "scale_model": {
        "title": "Scale Model",
        "arg": [
          {
            "name": "scale",
            "type": "num",
            "value": "1"
          }
        ]
      },
      "translate_model": {
        "title": "Translate Model",
        "arg": [
          {
            "name": "translate_x",
            "type": "num",
            "value": "0"
          },
          {
            "name": "translate_y",
            "type": "num",
            "value": "0"
          },
          {
            "name": "translate_z",
            "type": "num",
            "value": "0"
          }
        ]
      }
    }
  },
  "Visualization": {
    "id": "visualization",
    "blocks": {
      "show_in_viewer": {
        "title": "Show in 3D Viewer",
        "isStatementBlock": true,
        "arg": []
      },
      "clear_scene": {
        "title": "Clear Scene",
        "arg": []
      }
    }
  },
  "Stacked Layers": {
    "id": "stacked_layers",
    "blocks": {
      "generate_stacked_layers": {
        "title": "Generate Stacked Layers",
        "isStatementBlock": true,
        "arg": [
          {
            "name": "material_thickness",
            "type": "num",
            "value": "3"
          },
          {
            "name": "layer_count",
            "type": "num",
            "value": "10"
          }
        ]
      },
      "export_stacked_layers_svg": {
        "title": "Export Stacked Layers SVG",
        "arg": [
          {
            "name": "filename",
            "type": "str",
            "value": "stacked_layer"
          }
        ]
      }
